{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "prefer-const": "warn",
    "no-var": "warn",
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-undef": "error",
    "react/no-unescaped-entities": "warn",
    "react-hooks/rules-of-hooks": "warn"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "describe": "readonly",
    "it": "readonly",
    "expect": "readonly",
    "beforeEach": "readonly",
    "beforeAll": "readonly",
    "afterEach": "readonly",
    "afterAll": "readonly",
    "jest": "readonly",
    "fail": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "ignorePatterns": [
    "node_modules/",
    ".next/",
    "out/",
    "build/",
    "dist/"
  ]
}
